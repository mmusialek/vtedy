{
  "name": "vtedy",
  "version": "0.1.0",
  "private": true,
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "build": "webpack --mode production",
    "start2": "yarn concurrently  \"yarn scss:watch\" \"react-scripts start\"",
    "build2": "yarn concurrently \"scss:build\" \"react-scripts build\"",
    "eject": "react-scripts eject",
    "scss:watch": "node-sass-chokidar --watch src/styles/scss -o src/styles/css",
    "scss:build": "node-sass-chokidar src/styles/scss -o src/styles/css ",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  }
}
